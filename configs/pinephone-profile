alias r='sudo reboot'
alias pr='sudo apk del'
alias i='sudo apk add'
alias srp='source ~/.profile'
alias ms='mousepad'
alias mp='mousepad ~/.profile'
alias gac='cd ~/Documents/myscripts && git add . && git commit -m'
alias cdm='cd ~/Documents/myscripts'
alias gpm='cd ~/Documents/myscripts && git push -u origin main'
alias cpp='cp ~/.profile ~/Documents/myscripts/configs/pinephone-profile'
